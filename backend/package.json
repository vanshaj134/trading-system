{
  "name": "trading-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
  "setup": "npm ci && npx prisma generate && (npx prisma migrate dev --name init || npx prisma db push) && npx ts-node prisma/seed.ts && node ./scripts/fix-types.mjs",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "migrate": "prisma migrate dev --name init || true",
    "prisma:generate": "prisma generate",
    "test": "jest --passWithNoTests --coverage --runInBand"
  },
  "dependencies": {
    "fastify": "^4.23.0",
    "@prisma/client": "^5.0.0",
    "dotenv": "^16.0.0",
  "pino": "^8.14.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
  "@types/node": "^20.7.0",
  "jest": "^29.5.0",
  "ts-jest": "^29.0.5",
    "@types/jest": "^29.5.2"
  }
}